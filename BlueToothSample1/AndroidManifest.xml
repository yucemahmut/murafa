<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="com.vas.android.bluetooth.sample.sample1"
      android:versionCode="1"
      android:versionName="1.0">
    <application android:icon="@drawable/icon" android:label="@string/app_name" android:debuggable="true">
        <activity android:name=".BlueToothSample1"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            
</intent-filter>
        </activity>

    <activity android:label="Bluetooth Service Sample" android:screenOrientation="portrait" android:name="BluetoothServiceSample"><intent-filter><action android:name="com.vas.android.bluetooth.sample.sample1.SERVICE"></action>
<category android:name="android.intent.category.DEFAULT"></category>
</intent-filter>
</activity>

<activity android:name="LocalDeviceSample" android:label="LocalDeviceSample" android:screenOrientation="portrait"><intent-filter><action android:name="com.vas.android.bluetooth.sample.LOCAL"></action>
<category android:name="android.intent.category.DEFAULT"></category>
</intent-filter>
</activity>
</application>




<uses-permission android:name="android.permission.BLUETOOTH"></uses-permission>
<uses-permission android:name="android.permission.BLUETOOTH_ADMIN"></uses-permission>
</manifest> 